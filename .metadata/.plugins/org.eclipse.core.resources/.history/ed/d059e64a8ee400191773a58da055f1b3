import java.util.Scanner;

public class Floatvalue {

	public static void main(String[] args) {
		Scanner flvalue = new Scanner(System.in);
		System.out.println("Enter an Float value");
		float f = flvalue.nextFloat();
		System.out.println("value of f=="+f );
		Scanner ivalue = new Scanner(System.in);
		System.out.println("Enter an int value ");
		int i = ivalue.nextInt();
		System.out.println("value of i=="+i );
		System.out.println("Name is sabarish kanumuri);
		// TODO Auto-generated method stub

	}

}
